@model ValidatorGeocoder.Models.ValidatorExport

@{
    Layout = null;
    Response.ContentType = "application/vnd.ms-excel";
    Response.AddHeader("Content-Disposition", string.Format("attachment; filename={0}", Model.FileName));
}

<?xml version="1.0" encoding="utf-16"?>
<ss:Workbook xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet">
    
    <ss:Worksheet ss:Name="Validated Addresses">
        <ss:Table>
      
            if (savedProperty.USPScandidate_index.Any())
            {
            <ss:Row>
                <ss:Cell><ss:Data ss:Type="String">Google Street Address</ss:Data></ss:Cell>
          </ss:Row>
               }
            
            @foreach (var savedProperty in Model.SavedProperties)
            {
               <ss:Row>
                <ss:Cell><ss:Data ss:Type="String">@savedProperty.GoogleStreetAddress</ss:Data></ss:Cell> 
               </ss:Row> 
            }

        </ss:Table>
    </ss:Worksheet>

</ss:Workbook>