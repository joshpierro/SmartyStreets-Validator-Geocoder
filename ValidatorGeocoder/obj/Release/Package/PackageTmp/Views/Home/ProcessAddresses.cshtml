@model ValidatorGeocoder.Models.ValidatorExport

@{
    Layout = null;
    Response.ContentType = "application/vnd.ms-excel";
    Response.AddHeader("Content-Disposition", string.Format("attachment; filename={0}", Model.FileName));
}

<?xml version="1.0" encoding="utf-16"?>
<ss:Workbook xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet">
    
    <ss:Worksheet ss:Name="Validated Addresses">
        <ss:Table>
      
            <ss:Row>
                 <ss:Cell><ss:Data ss:Type="String">Id</ss:Data></ss:Cell>
                <ss:Cell><ss:Data ss:Type="String">USPScandidate_index</ss:Data></ss:Cell>
                <ss:Cell><ss:Data ss:Type="String">USPSdelivery_line_1</ss:Data></ss:Cell>
                <ss:Cell><ss:Data ss:Type="String">USPSlast_line</ss:Data></ss:Cell>
                <ss:Cell><ss:Data ss:Type="String">USPSprimary_number</ss:Data></ss:Cell>
                <ss:Cell><ss:Data ss:Type="String">USPSstreet_predirection</ss:Data></ss:Cell>
                <ss:Cell><ss:Data ss:Type="String">USPSstreet_name</ss:Data></ss:Cell>
                <ss:Cell><ss:Data ss:Type="String">USPSstreet_suffix</ss:Data></ss:Cell>
                <ss:Cell><ss:Data ss:Type="String">USPScity_name</ss:Data></ss:Cell>
                <ss:Cell><ss:Data ss:Type="String">USPSstate_abbreviation</ss:Data></ss:Cell>
                <ss:Cell><ss:Data ss:Type="String">USPSzipcode</ss:Data></ss:Cell>
                <ss:Cell><ss:Data ss:Type="String">USPSplus4_code</ss:Data></ss:Cell>
                <ss:Cell><ss:Data ss:Type="String">USPSrecord_type</ss:Data></ss:Cell>
                <ss:Cell><ss:Data ss:Type="String">USPSzip_type</ss:Data></ss:Cell>
                <ss:Cell><ss:Data ss:Type="String">USPScounty_fips</ss:Data></ss:Cell>
                <ss:Cell><ss:Data ss:Type="String">USPScounty_name</ss:Data></ss:Cell>
                <ss:Cell><ss:Data ss:Type="String">USPScarrier_route</ss:Data></ss:Cell>
                <ss:Cell><ss:Data ss:Type="String">USPScongressional_district</ss:Data></ss:Cell>
                <ss:Cell><ss:Data ss:Type="String">USPSrdi</ss:Data></ss:Cell>
                <ss:Cell><ss:Data ss:Type="String">USPSelot_sequence</ss:Data></ss:Cell>
                <ss:Cell><ss:Data ss:Type="String">USPSelot_sort</ss:Data></ss:Cell>
                <ss:Cell><ss:Data ss:Type="String">USPSdpv_match_code</ss:Data></ss:Cell>
                <ss:Cell><ss:Data ss:Type="String">USPSdpv_footnotes</ss:Data></ss:Cell>
                <ss:Cell><ss:Data ss:Type="String">USPSdpv_cmra</ss:Data></ss:Cell>
                <ss:Cell><ss:Data ss:Type="String">USPSdpv_vacant</ss:Data></ss:Cell>
                <ss:Cell><ss:Data ss:Type="String">USPSactive</ss:Data></ss:Cell>
                <ss:Cell><ss:Data ss:Type="String">USPSfootnotes</ss:Data></ss:Cell>
                <ss:Cell><ss:Data ss:Type="String">GoogleLatitude</ss:Data></ss:Cell>
                <ss:Cell><ss:Data ss:Type="String">GoogleLongitude</ss:Data></ss:Cell>
                <ss:Cell><ss:Data ss:Type="String">GoogleStreetAddress</ss:Data></ss:Cell>
                <ss:Cell><ss:Data ss:Type="String">GoogleCity</ss:Data></ss:Cell>
                <ss:Cell><ss:Data ss:Type="String">GoogleState</ss:Data></ss:Cell>
                <ss:Cell><ss:Data ss:Type="String">GoogleZip</ss:Data></ss:Cell>
                <ss:Cell><ss:Data ss:Type="String">GoogleCounty</ss:Data></ss:Cell>
                <ss:Cell><ss:Data ss:Type="String">GoogleLocationType</ss:Data></ss:Cell>
            </ss:Row>
            
            @foreach (var savedProperty in Model.SavedProperties)
            {
                <ss:Row>
                    <ss:Cell><ss:Data ss:Type="String">@savedProperty.Id</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@savedProperty.USPScandidate_index</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@savedProperty.USPSdelivery_line_1</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@savedProperty.USPSlast_line</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@savedProperty.USPSprimary_number</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@savedProperty.USPSstreet_predirection</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@savedProperty.USPSstreet_name</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@savedProperty.USPSstreet_suffix</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@savedProperty.USPScity_name</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@savedProperty.USPSstate_abbreviation</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@savedProperty.USPSzipcode</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@savedProperty.USPSplus4_code</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@savedProperty.USPSrecord_type</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@savedProperty.USPSzip_type</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@savedProperty.USPScounty_fips</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@savedProperty.USPScounty_name</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@savedProperty.USPScarrier_route</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@savedProperty.USPScongressional_district</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@savedProperty.USPSrdi</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@savedProperty.USPSelot_sequence</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@savedProperty.USPSelot_sort</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@savedProperty.USPSdpv_match_code</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@savedProperty.USPSdpv_footnotes</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@savedProperty.USPSdpv_cmra</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@savedProperty.USPSdpv_vacant</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@savedProperty.USPSactive</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@savedProperty.USPSfootnotes</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@savedProperty.GoogleLatitude</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@savedProperty.GoogleLongitude</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@savedProperty.GoogleStreetAddress</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@savedProperty.GoogleCity</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@savedProperty.GoogleState</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@savedProperty.GoogleZip</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@savedProperty.GoogleCounty</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@savedProperty.GoogleLocationType</ss:Data></ss:Cell> 
                </ss:Row> 
            }

        </ss:Table>
    </ss:Worksheet>
    
    <ss:Worksheet ss:Name="Rejected Addresses">
        <ss:Table>
      
            <ss:Row>
                <ss:Cell><ss:Data ss:Type="String">Id</ss:Data></ss:Cell>
                <ss:Cell><ss:Data ss:Type="String">Address</ss:Data></ss:Cell>
                <ss:Cell><ss:Data ss:Type="String">City</ss:Data></ss:Cell>
                <ss:Cell><ss:Data ss:Type="String">State</ss:Data></ss:Cell>
                <ss:Cell><ss:Data ss:Type="String">Zip</ss:Data></ss:Cell>  
            </ss:Row>
            
            @foreach (var rejectedProperty in Model.RejectedProperties)
            {
               <ss:Row>
                   <ss:Cell><ss:Data ss:Type="String">@rejectedProperty.Id</ss:Data></ss:Cell>
                   <ss:Cell><ss:Data ss:Type="String">@rejectedProperty.StreetAddress</ss:Data></ss:Cell>
                   <ss:Cell><ss:Data ss:Type="String">@rejectedProperty.City</ss:Data></ss:Cell>
                   <ss:Cell><ss:Data ss:Type="String">@rejectedProperty.State</ss:Data></ss:Cell>
                   <ss:Cell><ss:Data ss:Type="String">@rejectedProperty.Zipcode</ss:Data></ss:Cell>
               </ss:Row> 
            }

        </ss:Table>
    </ss:Worksheet>

</ss:Workbook>
